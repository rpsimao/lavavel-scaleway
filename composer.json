{
    "name": "rpsimao/laravel-scaleway",
    "type": "library",
    "description" : "Library for interacting with the Scaleway REST API",
    "keywords": ["laravel", "rest", "scaleway"],
    "homepage": "https://github.com/rpsimao/laravel-scaleway",
    "license": "MIT",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0"

    },
    "autoload": {
        "psr-4": {
            "rpsimao\\RPSScaleway\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "orchestra/testbench": "^3.4"
    },
    "autoload-dev": {
      "psr-4": {
        "rpsimao\\RPSScaleway\\Test\\": "tests/"
      }
  }
}
